#include <stdio.h>
#include <math.h>
int main(void)
{
    int no,a,b;
    printf("please en a num");
    scanf("%d",&no);
    for(a=1;a<=no;a++) {
        b=pow(2,a);
        if(b<no)
            printf("%d\t",b);
    }
    
    return 0;
  
        
    
    
}
