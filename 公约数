#include <stdio.h>
int main(void)
{
    int a,b,c,i;
   
    printf("please enter two numbers\n");
    scanf("%d",&a);
    scanf("%d",&b);
   
    if (a<=b)
        c=a;
    else
        c=b;
    for (i=1;i<=c;i++)
    {
        if (a%i==0&&b%i==0)
        printf("%3d",i);
    }
        
    
   
    
  
    return 0;
    
}
