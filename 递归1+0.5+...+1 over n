#include <stdio.h>

float fac(int n)
{

    if(n==1)

        return 1;

else

return 1/fac(n)+1/fac(n-1);
}

int main(void)
{
    int num;
    scanf("%d\n",&num);

    printf("%.f",fac(num));
return 0;
}
