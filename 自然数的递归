#include <stdio.h>

int f(int n)
{
  
    if (n==0)
        return 0;
    else
        printf("%d\n",f(n-1)+1);
    return n;
}






int main(void){
    printf("wait...soon\n");
    int num=50;
    printf("the answer is  %d\n",f(num));
    
    return 0;
    
}
